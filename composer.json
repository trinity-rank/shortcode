{
    "name": "trinityrank/shortcode",
    "description": "Laravel Shortcodes",
    "keywords": [
        "trinityrank",
        "shortcode"
    ],
    "homepage": "https://github.com/trinity-rank/shortcode",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "TrinityRank DevTeam",
            "email": "trinitydevbg@gmail.com",
            "role": "Team"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Trinityrank\\Shortcodes\\": "src",
            "Trinityrank\\Shortcodes\\Compilers": "src/Compilers",
            "Trinityrank\\Shortcodes\\View": "src/View",
            "Trinityrank\\Shortcodes\\Facades": "src/Facades"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinityrank\\Shortcodes\\ShortcodesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
